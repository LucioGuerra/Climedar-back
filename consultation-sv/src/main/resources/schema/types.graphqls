type ConsultationModel @key(fields: "id") {
    id: ID!
    date: String!
    startTime: String!
    estimatedDuration: String!
    description: String!
    medicalServices: MedicalServicesModel!
    finalPrice: Float!
    observation: String
    doctor: DoctorModel!
    patient: PatientModel!
}

type DoctorModel @key(fields: "id") @extends {
    id: ID! @external
}

type PatientModel @key(fields: "id") @extends{
    id: ID! @external
}

interface MedicalServicesModel @key(fields: "id") @extends {
    id: ID! @external
}

enum ServiceType {
    GENERAL
    SPECIALIST
    SURGERY
    EXAMS
    THERAPY
}

type PageInfo {
    currentPage: Int!
    totalPages: Int!
    totalItems: Int!
    itemsPerPage: Int!
    hasNext: Boolean!
    hasPrevious: Boolean!
}

type ConsultationPage {
    pageInfo: PageInfo!
    consultations: [ConsultationModel]!
}