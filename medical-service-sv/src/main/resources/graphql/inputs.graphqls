input PageRequest {
    page: Int!
    size: Int!
    order: Order
}

input Order {
    field: String!
    direction: SortDirection!
}

enum SortDirection {
    ASC
    DESC
}

input MedicalServiceInput {
    name: String!
    description: String!
    price: Float!
    serviceType: ServiceType!
    specialityId: ID!
}

input MedicalSpecificationInput {
    code: String
    name: String
    description: String
    serviceType: ServiceType
    specialityId: ID
}
