type Mutation{
    createMedicalService(input: MedicalServiceInput!): MedicalService!
    updateMedicalService(id: ID!, input: MedicalServiceInput!): MedicalService!
    deleteMedicalService(id: ID!): Boolean!

    createMedicalPackage(servicesIds: [ID!]!): MedicalPackage!
    addServiceToMedicalPackage(id: ID!, serviceId: ID!): MedicalPackage!
    removeServiceFromMedicalPackage(id: ID!, serviceId: ID!): MedicalPackage!
    deleteMedicalPackage(id: ID!): Boolean!
}